    <xsd:sequence>
      <xsd:element name="Type" type="trk:CommonCodeDescriptionType" />
      <xsd:element name="Address" type="trk:AddressType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="AppointmentType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Made" type="trk:DateTimeType" />
