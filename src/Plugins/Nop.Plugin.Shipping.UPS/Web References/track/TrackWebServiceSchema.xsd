      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Document" type="trk:DocumentType" />
      <xsd:element minOccurs="0" name="FileNumber" type="xsd:string" />
      <xsd:element minOccurs="0" name="Appointment" type="trk:AppointmentType" />
