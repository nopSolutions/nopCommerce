@model AppSettingsModel

<div class="card-body">
    <div class="form-group row">
        <div class="col-md-3">
            <nop-label asp-for="CacheConfigModel.DefaultCacheTime" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="CacheConfigModel.DefaultCacheTime" />
            <span asp-validation-for="CacheConfigModel.DefaultCacheTime"></span>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            <nop-label asp-for="CacheConfigModel.ShortTermCacheTime" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="CacheConfigModel.ShortTermCacheTime" />
            <span asp-validation-for="CacheConfigModel.ShortTermCacheTime"></span>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            <nop-label asp-for="CacheConfigModel.BundledFilesCacheTime" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="CacheConfigModel.BundledFilesCacheTime" />
            <span asp-validation-for="CacheConfigModel.BundledFilesCacheTime"></span>
        </div>
    </div>
</div>