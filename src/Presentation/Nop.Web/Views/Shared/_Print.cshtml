@using Nop.Core
@using Nop.Services.Localization
@inject IThemeContext themeContext
@inject IWorkContext workContext;
@{
    var themeName = themeContext.WorkingThemeName;
}
<!DOCTYPE html>
<html lang="@workContext.WorkingLanguage.GetTwoLetterIsoLanguageName()" @Html.NopPageCssClasses()>
<head>
    <title>@Html.NopTitle()</title>
    <meta http-equiv="Content-type" content="text/html;charset=UTF-8" />
    <link href="@Url.Content($"~/Themes/{themeName}/Content/css/print.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/lib/jquery/jquery-3.3.1.min.js")"></script>
    <script src="@Url.Content("~/lib/jquery-migrate/jquery-migrate-3.0.1.js")"></script>
    @Html.NopInlineScripts(this.Url, ResourceLocation.Head)
</head>
<body>
    @RenderBody()
    @Html.NopInlineScripts(this.Url, ResourceLocation.Footer)
</body>
</html>
