@model IList<PollModel>

@if (Model.Count > 0)
{
    <div class="home-page-polls todays-poll-box row">
        <div class=" col-md-12">
            <div class="title panel-heading">
                <h2>@T("Polls.Title")</h2>
            </div>
        </div>
        <div class="col-md-12">
            @foreach (var poll in Model)
            {
                @await Html.PartialAsync("_Poll", poll)
            }
        </div>
</div>
}
