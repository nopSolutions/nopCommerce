    //Kendo drawer widget
    <script src="https://kendo.cdn.telerik.com/2021.2.616/js/kendo.core.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2021.2.616/js/kendo.userevents.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2021.2.616/js/kendo.drawer.min.js"></script>

