    <script src="https://onjobsupport.blob.core.windows.net/nop-cdn/js/bootstrap.min.js"></script>
    <script src="https://onjobsupport.blob.core.windows.net/nop-cdn/js/head.min.js"></script>
    <script src="https://onjobsupport.blob.core.windows.net/nop-cdn/js/owl.carousel.min.js"></script>
    <script src="https://onjobsupport.blob.core.windows.net/nop-cdn/js/public.countryselect.js"></script>
    <script src="https://onjobsupport.blob.core.windows.net/nop-cdn/js/public.ajaxcart.js"></script>
