	</filter>

	<use var="HTTP_X_UP_DEVCAP_SOFTKEYSIZE" as="x_up_devcap_softkeysize" />
	<filter>
		<case match="(?'softkeySize'\d+)" with="%{x_up_devcap_softkeysize}">
