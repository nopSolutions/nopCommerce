		</case>

		<!-- Netscape -->
		<case match="^Mozilla/(?'version'(?'major'\d+)(?'minor'\.\d+)(?'letters'\w*)).*">
			browser=Netscape
			version=${version}
			majorversion=${major}
			minorversion=${minor}

			<filter match="^b" with="${letters}">
				beta=true
			</filter>
			<filter match="Gold" with="${letters}">
				gold=true
			</filter>
			<filter>
				<case match="^[4-9]\." with="${version}">
					frames=true
					tables=true
					cookies=true
					javascript=true
					javaapplets=true
					ecmascriptversion=1.2
					css1=true

					<filter match="^[5-9]*" with="${minor}">
