					</filter>
				</case>
				<case match="Mozilla/.*\(compatible; MMEF(?'versionInfo'\d\d); Cell[pP]hone(([;,] (?'deviceID'[^;]*))(;(?'buildInfo'.*))*)*\)">

					canCombineFormsInDeck = "false"
					canRenderPostBackCards = "false"

					<filter match="(?'majorVersion'\d)(?'minorVersion'\d)" with="${versionInfo}">
