		<case match="(?'Browser'^\w+)/(?'version'(?'major'\d+)\.(?'minor'\d+))">
			browser=${Browser}
			version=${version}
			majorversion=${major}
			minorversion=${minor}
			Unknown=true
		</case>
	</filter>

