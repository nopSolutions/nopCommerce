{
  "enabled": "true",
  "increase-coverage": {
    "project-name": "dotnet-nopcommerce",
    "target-branches": [
      "main"
    ],
    "test-env-docker-stage": "test",
    "build-test-env-docker-stage-script-path": "tool/build-test-env-docker-stage.sh",
    "test-files-folders": [
      "src/Tests/Nop.Tests/Nop.Services.Tests/Tax/TaxServiceTests.cs"
    ],
    "source-test-file-paths": [
      {
        "source-file-path": "src/Libraries/Nop.Services/Tax/TaxPluginManager.cs",
        "test-file-path": "src/Tests/Nop.Tests/Nop.Services.Tests/Tax/TaxServiceTests.cs"
      }
    ],
    "test-command": "dotnet test src/Tests/Nop.Tests/Nop.Tests.csproj --configuration Release --logger trx --collect:\"XPlat Code Coverage\" --results-directory TestResults/",
    "coverage-type": "cobertura",
    "code-coverage-report-path": "TestResults/*/coverage.cobertura.xml",
    "desired-coverage": 85,
    "max-iterations": 3,
    "max-number-of-test-cases": 6
  }
}